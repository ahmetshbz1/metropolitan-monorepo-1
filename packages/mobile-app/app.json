{
  "expo": {
    "name": "metropolitan",
    "slug": "metropolitan",
    "version": "1.0.9",
    "hooks": {
      "postInstall": "cd ../shared && (bun run build || npm run build || yarn build) || true"
    },
    "orientation": "portrait",
    "icon": "./assets/images/icooon.png",
    "scheme": "metropolitan",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icooon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "locales": {
      "tr": "./locales/native/tr.json",
      "pl": "./locales/native/pl.json",
      "en": "./locales/native/en.json"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.metropolitan.food",
      "buildNumber": "47",
      "deploymentTarget": "18.0",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.18493082891-0qg1bp44jncs4m7jt6426ud1cv7bil4t"
        }
      },
      "infoPlist": {
        "LSApplicationCategoryType": "public.app-category.food-and-drink",
        "CFBundleAllowMixedLocalizations": true,
        "CFBundleLocalizations": ["tr", "en", "pl"],
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.18493082891-0qg1bp44jncs4m7jt6426ud1cv7bil4t",
              "com.metropolitan.food"
            ]
          }
        ]
      },
      "appleTeamId": "64XKK46655",
      "entitlements": {
        "com.apple.developer.in-app-payments": [
          "merchant.com.metropolitan.food"
        ],
        "com.apple.developer.applesignin": ["Default"],
        "aps-environment": "production"
      },
      "usesAppleSignIn": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icooon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.metropolitan.food",
      "versionCode": 6,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "com.google.android.c2dm.permission.RECEIVE"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-font",
      "expo-localization",
      "expo-image-picker",
      "expo-apple-authentication",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icooon.png",
          "color": "#ffffff",
          "defaultChannel": "default"
